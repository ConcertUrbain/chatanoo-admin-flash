package org.concert_urbain.touratour.events{	import flash.events.Event;		public class IdEvent extends Event	{			public static var CHANGE:String = "org.concert_urbain.touratour.events.idevent.change";		public static var DELETE:String = "org.concert_urbain.touratour.events.idevent.delete";		public static var ADD:String = "org.concert_urbain.touratour.events.idevent.add";				public var id:Number;		public var data:*;			public function IdEvent(type:String, msgId:Number=0, bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);						id = msgId;		}				override public function clone():Event		{			return new IdEvent(type, id, bubbles, cancelable) ;		}	}}
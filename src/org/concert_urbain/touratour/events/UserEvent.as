package org.concert_urbain.touratour.events{	import flash.events.Event;		public class UserEvent extends Event	{			public static var DISCONNECT:String = "org.concert_urbain.touratour.events.userevent.disconnect";		public static var AUTHENTIFY:String = "org.concert_urbain.touratour.events.userevent.authentify";		public static var NOT_AN_ADMIN:String = "org.concert_urbain.touratour.events.userevent.notanadmin";		public static var CHANGE:String = "org.concert_urbain.touratour.events.userevent.change";		public static var DELETE:String = "org.concert_urbain.touratour.events.userevent.delete";		public static var ADD:String = "org.concert_urbain.touratour.events.userevent.add";				public var id:Number;			public function UserEvent(type:String, userId:Number=0, bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);						id = userId;		}				override public function clone():Event		{			return new UserEvent(type, id, bubbles, cancelable) ;		}	}}
package org.concert_urbain.utils{	import flash.events.Event;		public class NumberEvent extends Event	{		public var number:Number;		public function NumberEvent(type:String, aNumber:Number, bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);			number = aNumber;		}				override public function clone():Event		{			return new NumberEvent(type, number, bubbles, cancelable) ;		}	}}
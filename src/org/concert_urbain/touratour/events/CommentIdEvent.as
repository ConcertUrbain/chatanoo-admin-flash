package org.concert_urbain.touratour.events{	import flash.events.Event;		public class CommentIdEvent extends Event	{			private var _itemId:Number;		private var _commentId:Number;			public function CommentIdEvent(type:String, itemId:Number, commentId:Number, bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);						_itemId = itemId;			_commentId = commentId;		}		public function get itemId():Number		{			return _itemId;		}		public function get commentId():Number		{			return _commentId;		}				override public function clone():Event		{			return new CommentIdEvent(type, _itemId, _commentId, bubbles, cancelable) ;		}	}}
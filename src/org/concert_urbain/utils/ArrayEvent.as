package org.concert_urbain.utils{	import flash.events.Event;		public class ArrayEvent extends Event {				public var array:Array;		public function ArrayEvent(type:String, data:Array, bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);						array = data;		}				override public function clone():Event		{			return new ArrayEvent(type, array, bubbles, cancelable) ;		}	}}
package org.concert_urbain.utils{	import flash.events.Event;	import flash.net.NetStream;		public class MediaEvent extends Event {				public var mediaType:String;		public var mediaUrl:String;		public function MediaEvent(type:String, mediatype:String, url:String, bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);						mediaType = mediatype;			mediaUrl = url;		}				override public function clone():Event		{			return new MediaEvent(type, mediaType, mediaUrl, bubbles, cancelable) ;		}	}}
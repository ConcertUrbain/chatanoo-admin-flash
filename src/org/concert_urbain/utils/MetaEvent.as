package org.concert_urbain.utils{	import flash.events.Event;		import org.concert_urbain.touratour.api.vos.*;	import org.concert_urbain.touratour.api.vos.datas.*;	public class MetaEvent extends Event {				public var meta:Meta;		public function MetaEvent(type:String, aMeta:Meta, bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);						meta = aMeta;		}				override public function clone():Event		{			return new MetaEvent(type, meta, bubbles, cancelable) ;		}	}}
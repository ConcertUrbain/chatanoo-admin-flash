package org.concert_urbain.utils{	import flash.events.Event;		public class StringEvent extends Event	{		public var string:String;		public function StringEvent(type:String, aString:String, bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);			string = aString		}				override public function clone():Event		{			return new StringEvent(type, string, bubbles, cancelable) ;		}	}}
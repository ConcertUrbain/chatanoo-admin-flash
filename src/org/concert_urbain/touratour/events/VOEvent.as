package org.concert_urbain.touratour.events{	import flash.events.Event;		public class VOEvent extends Event	{		public static var ROLL:String = "org.concert_urbain.touratour.events.voevent.roll";		public static var CHANGE:String = "org.concert_urbain.touratour.events.voevent.change";		public static var DELETE:String = "org.concert_urbain.touratour.events.voevent.delete";		public static var ADD:String = "org.concert_urbain.touratour.events.voevent.add";		public static var SELECT:String = "org.concert_urbain.touratour.events.voevent.select";		public static var MULTIPLE_SELECT:String = "org.concert_urbain.touratour.events.voevent.multipleselect";				public var vo:Object;		public var param:String;			public function VOEvent(type:String, o:Object, bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);						vo = o;		}				override public function clone():Event		{			return new VOEvent(type, vo, bubbles, cancelable) ;		}	}}
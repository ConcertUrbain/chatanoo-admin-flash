package org.concert_urbain.utils{	import flash.events.Event;		public class IntEvent extends Event	{			public static var CHANGE:String = "org.concert_urbain.utils.intevent.change";		public static var DELETE:String = "org.concert_urbain.utils.intevent.delete";		public static var ADD:String = "org.concert_urbain.utils.intevent.add";				public var integer:int;					public function IntEvent(type:String, intValue:int=0, bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);						integer = intValue;		}				override public function clone():Event		{			return new IntEvent(type, integer, bubbles, cancelable) ;		}	}}
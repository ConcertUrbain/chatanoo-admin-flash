package com.tralalere.events{	import flash.events.Event;		public class XMLEvent extends Event	{		public var xml:XML;		public function XMLEvent(type:String, aXML:XML, bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);			xml = aXML		}				override public function clone():Event		{			return new XMLEvent(type, xml, bubbles, cancelable) ;		}	}}
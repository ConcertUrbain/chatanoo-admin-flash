package org.concert_urbain.utils{	import flash.events.Event;	import flash.display.DisplayObject;			public class DisplayObjectEvent extends Event	{			public var object:DisplayObject;			public function DisplayObjectEvent(type:String, o:DisplayObject, bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);			object = o;		}				override public function clone():Event		{			return new DisplayObjectEvent(type, object, bubbles, cancelable) ;		}	}}